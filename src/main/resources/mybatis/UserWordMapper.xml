<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="samuragi.enou.repository.ext.UserWordExt">
    <update id="update" parameterType="samuragi.enou.dto.dtodb.DtoDbUserWord">
    update user_word set
        word = #{word}
    where id = #{id}
    </update>
</mapper>